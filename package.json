{
  "name": "drive-vector-sync-cf",
  "version": "1.0.0",
  "description": "Google Drive to Qdrant Vector Sync System on Cloudflare Workers",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "prepare": "husky"
  },
  "keywords": [
    "cloudflare-workers",
    "rag",
    "vector-database",
    "google-drive",
    "qdrant",
    "embeddings"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2",
    "vitest": "^2.1.5",
    "wrangler": "^3.87.0"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.12.0",
    "googleapis": "^166.0.0",
    "openai": "^4.76.1",
    "pdfjs-dist": "^4.10.38",
    "tiktoken": "^1.0.18"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "vitest related --run"
    ]
  }
}
